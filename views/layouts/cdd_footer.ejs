<script type="text/javascript">
    var quill = false;
    $(document).on("click", ".poster", function() {
        var data = {};
        data.tipo = $(this).data('tipo');
        $(".modal-footer #tipo").val(data.tipo);
        switch(data.tipo) {
            case 1:
                $("#t_princ").attr("placeholder","Paz Mundial");
                break;
            case 2:
                $("#t_princ").attr("placeholder","Título");

                break;
            case 3:
                $("#t_princ").attr("placeholder","Url Imagen");
                break;
            case 4:
                $("#t_princ").attr("placeholder","Url Video (sólo youtube)");
                break;
            default:
                status = "inactivo";
        }
        if(parseInt(data.tipo) > 1){
            $(".modal-footer #tag").removeClass("hidden");
            if(!quill){
                $("#editor").jqte({
                    color: false,
                    source: false,
                    sub: false,
                    sup: false,
                    strike: false,
                    indent: false,
                    format: false,
                    outdent: false
                });
                quill = true;
            }
        } else {
            $(".modal-footer #tag").addClass("hidden");
            $("#editor").jqte({
                color: false,
                source: false,
                sub: false,
                sup: false,
                strike: false,
                indent: false,
                format: false,
                outdent: false
            });
            quill = false;
        }
    });

    $.material.init();
    $('body').materialScrollTop();
</script>

